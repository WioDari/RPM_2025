<Window x:Class="Musick.MainWindow"
        Style="{StaticResource GeneralStyle}"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Musick"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <TextBlock  HorizontalAlignment="Left" Margin="27,44,0,0" TextWrapping="Wrap" Text="{Binding AllAlbums}" VerticalAlignment="Top"/>
        <TextBlock  HorizontalAlignment="Left" Margin="27,63,0,0" TextWrapping="Wrap" Text="{Binding AllTrack}" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="27,82,0,0" TextWrapping="Wrap" Text="{Binding AllPlay}" VerticalAlignment="Top"/>

    </Grid>
</Window>
